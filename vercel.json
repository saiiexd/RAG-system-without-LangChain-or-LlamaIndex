{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "main.py"
    },
    {
      "src": "/ask",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "static/$1"
    }
  ]
}
